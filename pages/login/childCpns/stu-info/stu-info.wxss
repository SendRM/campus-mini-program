.stu-info {
  margin: 70rpx 0 90rpx 0;
  padding: 0 10rpx;
}
.sno,
.pwd {
  position: relative;
}
.stu-info input {
  height: 80rpx;
  padding: 0 0 10rpx 70rpx;
  border-bottom: 1rpx solid #cccc;
  text-align: left;
}
.stu-id::before {
  top: 37%;
  background: url("data:image/svg+xml;base64,") no-repeat;
}
.stu-pwd::before {
  background: url("data:image/svg+xml;base64,");
}
.stu-ipt::before {
  content: "";
  position: absolute;
  left: 1rpx;
  top: 4rpx;
  width: 60rpx;
  height: 60rpx;
  background-size: contain;
}
.clearSno,
.clearPwd {
  width: 45rpx;
  height: 45rpx;
  top: 22%;
  right: 15rpx;
  background-image: url("data:image/svg+xml;charset=utf-8;base64,");
  z-index: 99;
}
.stu-pwd {
  margin-top: 30rpx;
}
.login-btn {
  width: 400rpx;
  color: white;
  background-color: #005CA1;
}
