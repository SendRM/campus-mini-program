.wrap {
  position: sticky;
  position: -webkit-sticky;
  top: 0rpx;
  background-color: #fff;
}
.type {
  position: relative;
  color: #6F6F6F;
  font-size: 34rpx;
}
.type::after {
  position: absolute;
  top: 14rpx;
  right: -29rpx;
  content: '';
  width: 12rpx;
  height: 12rpx;
  border-left: 4rpx solid #6F6F6F;
  border-bottom: 4rpx solid #6F6F6F;
  transform: rotate(-45deg);
}
.more {
  width: 54rpx;
  height: 54rpx;
  right: 40rpx;
  background-image: url("data:image/svg+xml;charset=utf-8;base64,");
}
.search {
  width: 90%;
  padding-bottom: 10rpx;
}