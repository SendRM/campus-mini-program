/* pages/find/childCpns/dynamic/dynamic.wxss */
.user {
  display: flex;
  flex-direction:row;
  align-items: center;
  flex-wrap: wrap;
  width:700rpx;
  margin: 0 auto;
  padding: 30rpx 0 10rpx;
  border-top: 10rpx solid #F6F6F6;
  border-bottom: 10rpx solid #F6F6F6;
}
.user-info {
  font-size: 32rpx;
  color: #B2B2B2;
}
.belong {
  padding-left: 20rpx;
}
.college {
  color: #2775E3;
}
.content {
  width: 100%;
}
.content text{
  display: inline-block;
  margin: 20rpx 0 20rpx 10rpx;
  padding-left: 10rpx;
}
.content image {
  width: 100%;
  margin: 0 10rpx;
  border-radius: 20rpx;
}
.user-img {
  display: flex;
  flex: 1;
  width: 100%;
}
.user-img image {
  width: 33.3%;
  height: 220rpx;
}
.click {
  margin-left: 80%;
  height: 40rpx;
  position: relative;
}
.icon {
  top: -6rpx;
}
.like {
  width: 47rpx;
  height: 47rpx;
  top: -14rpx;
  right: 90rpx;
}
.default {
  background-image: url("data:image/svg+xml;charset=utf-8;base64,");
}
.choose {
  background-image: url("data:image/svg+xml;charset=utf-8;base64,");
}
.comment {
  width: 40rpx;
  height: 40rpx;
  right: -48rpx;
  background-image: url("data:image/svg+xml;charset=utf-8;base64,");
}
.count {
  position: absolute;
  top: 0rpx;
  left: 0;
  font-size: 32rpx;
  color: #9B9B9B;
}
.count-comment {
  margin: -8rpx 0 0 70rpx;
}
.count-like {
  margin: -8rpx 0 0 -70rpx;
}
.release{
  font-size: 14px;
  background-color: #fff;
  display: flex;
  align-items: flex-end;
  justify-content: space-between;
  box-sizing: border-box;
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  padding: 5px 0 5px 15px;
}
.releaseInput{
  color: #ccc;
}
.release .text{
  width: 302px;
  min-height: 17px;
  max-height: 51px;
  line-height: 17px;
  border-width: 7px 10px;
  border-style: solid;
  border-color: #fff;
  background-color: #fff;
}
.release .submit{
  color: #2775E3;
  width: 58px;
  height: 32px;
  line-height: 32px;
  text-align: center;
}